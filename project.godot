[application]

config/name="Casino Defender"
run/main_scene="res://scenes/splash/splash_logo.tscn"
run/disable_stdout=true
run/disable_stderr=true
boot_splash/image="res://textures/post-fx/black.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

math="*res://scripts/common/math.gd"
constants="*res://scripts/common/constants.gd"
global="*res://scripts/common/global.gd"

[debug]

gdscript/warnings/enable=false

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false
window/size/test_width=1280
window/size/test_height=720
window/vsync/vsync_via_compositor=true
window/stretch/aspect="expand"
window/tablet_driver=""

[editor]

convert_text_resources_to_binary_on_export=true

